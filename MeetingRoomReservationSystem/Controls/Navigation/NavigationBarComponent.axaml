<UserControl x:Class="MeetingRoomReservationSystem.Controls.Navigation.NavigationBarComponent"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:CompileBindings="True"
             xmlns:vm="clr-namespace:MeetingRoomReservationSystem.Controls.Navigation"
             x:DataType="vm:NavigationBarVM"
             d:DesignHeight="300" d:DesignWidth="300">
    
    <Grid ColumnDefinitions="Auto Auto" ColumnSpan="20">
        <Button Grid.Column="0"
            Command="{Binding NavigateToCommand}"
            Content="Создать встречу"/>
        
        <Button  Grid.Column="1"
                 Command="{Binding NavigateBackCommand}"
                 Content="Комнаты переговора" />
    </Grid>
</UserControl>
